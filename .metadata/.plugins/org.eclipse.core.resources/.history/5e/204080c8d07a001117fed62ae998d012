import javax.swing.*;
import java.awt.*;
public class Barra  {


	
	public static void main(String[] args){
		final int MAX = 100;
		final JFrame frame = new JFrame("Barra de progreso");
		// crear barra
		final JProgressBar pb = new JProgressBar();
		
		pb.setMinimum(0);
		pb.setMaximum(MAX);
		pb.setStringPainted(true);
		
		// agregar barra
		
		
		
		
	}
	
	
	
	
	
	
}
